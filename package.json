{
  "name": "caddy-npm",
  "version": "2.0.0-rc.1",
  "description": "Install caddy with npm",
  "repository": {
    "type": "git",
    "url": "https://github.com/RecuencoJones/caddy-npm"
  },
  "scripts": {
    "postinstall": "go-npm install",
    "preuninstall": "go-npm uninstall"
  },
  "goBinary": {
    "name": "caddy",
    "path": "./bin",
    "url": {
      "linux": {
        "386": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_Linux_i386.tar.gz",
        "amd64": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_Linux_x86_64.tar.gz",
        "arm": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_Linux_armv7.tar.gz",
        "arm64": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_Linux_arm64.tar.gz"
      },
      "freebsd": {
        "386": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_freebsd_i386.tar.gz",
        "amd64": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_freebsd_x86_64.tar.gz",
        "arm": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_freebsd_armv7.tar.gz",
        "arm64": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_freebsd_arm64.tar.gz"
      },
      "darwin": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_macOS_x86_64.tar.gz",
      "windows": {
        "386": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_Windows_i386.zip",
        "amd64": "https://github.com/caddyserver/caddy/releases/download/v{{version}}/caddy_{{version}}_Windows_x86_64.zip"
      }
    }
  },
  "dependencies": {
    "go-npm": "RecuencoJones/go-npm#8ca865460b2b1b04d9ffe7f777e2569b6318010a"
  }
}
