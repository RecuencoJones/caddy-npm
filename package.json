{
  "name": "caddy-npm",
  "version": "2.0.0-beta.15",
  "description": "Install caddy with npm",
  "repository": {
    "type": "git",
    "url": "https://github.com/RecuencoJones/caddy-npm"
  },
  "scripts": {
    "postinstall": "go-npm install",
    "preuninstall": "go-npm uninstall"
